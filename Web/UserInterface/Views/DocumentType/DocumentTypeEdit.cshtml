@section Scripts{

    <script type="text/javascript">
        $(document).ready(function () {
            $('#frmDocumentTypeEdit').validator().on('submit', function (e) {
                if (e.isDefaultPrevented()) {
                    // handle the invalid form...
                }
                else {
                    var obj = {
                        Id: @Model.Id,
                        BillType: $("#BillType").val(),
                        Document_Code: $("#Document_Code").val(),
                        OurSoftwareProcessing: $("#OurSoftwareProcessing").val(),
                        Document_Type: $("#Document_Type").val(),
                        Ip: $("#Ip").val(),
                        BrowserCase: $("#BrowserCase").val()
                    };
                    e.preventDefault();
                    $.ajax({
                        url: '/DocumentType/DocomentTypeEdits',
                        method: 'POST',
                        data: obj,
                        success: function (data) {
                            debugger;
                            $.toast({
                                text: "Document Type Update SuccessFully",
                                position: 'top-right',
                                loaderBg: '#f8b32d',
                                icon: 'success',
                                hideAfter: 3500,
                                stack: 6,

                            })
                            setTimeout(function () {
                                window.location.href = "/DocumentType/DocumentTypeList"
                            }, 1500);
                        }
                    });
                }
            });
        });
    </script>
    }